    "express": "^4.21.2",
    "graphql": "^16.10.0",
    "graphql-config": "^5.1.3",
    "graphql-query-complexity": "^1.1.0",
    "graphql-scalars": "^1.24.2",
    "graphql-upload": "^17.0.0",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.10.0",
    "reflect-metadata": "^0.2.2",
    "type-graphql": "^2.0.0-rc.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
